<?xml version="1.0" ?>
<robot name="hybrid_goalie_robot">

	<!-- Base -->
	<link name="base_link">
		<visual>
			<geometry>
				<capsule radius="0.25" length="0.9"/>
			</geometry>
			<origin xyz="0 0 0.5" rpy="0 0 0"/>
			<material name="steel">
				<color rgba="0.3 0.3 0.3 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<capsule radius="0.25" length="0.9"/>
			</geometry>
			<origin xyz="0 0 0.5" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="20"/>
			<inertia ixx="0.533" iyy="0.467" izz="0.133" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<!-- LEFT LEG -->
	<joint name="left_leg_joint" type="fixed">
		<parent link="base_link"/>
		<child link="left_leg"/>
		<origin xyz="0.12 0 -0.2" rpy="0 0 0"/>
	</joint>

	<link name="left_leg">
		<visual>
			<geometry>
				<cylinder radius="0.05" length="0.4"/>
			</geometry>
			<origin xyz="0 0 -0.2" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.05" length="0.4"/>
			</geometry>
			<origin xyz="0 0 -0.2" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="1"/>
			<inertia ixx="0.01" iyy="0.01" izz="0.01" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<!-- RIGHT LEG -->
	<joint name="right_leg_joint" type="fixed">
		<parent link="base_link"/>
		<child link="right_leg"/>
		<origin xyz="-0.12 0 -0.2" rpy="0 0 0"/>
	</joint>

	<link name="right_leg">
		<visual>
			<geometry>
				<cylinder radius="0.05" length="0.4"/>
			</geometry>
			<origin xyz="0 0 -0.2" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.05" length="0.4"/>
			</geometry>
			<origin xyz="0 0 -0.2" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="1"/>
			<inertia ixx="0.01" iyy="0.01" izz="0.01" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>


	<!-- LEFT ARM -->

	<!-- Left Shoulder -->
	<joint name="left_shoulder" type="revolute">
		<parent link="base_link"/>
		<child link="left_upper_arm"/>
		<origin xyz="0.3 0 0.7" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<limit lower="-3.14" upper="3.14" effort="100" velocity="2.0"/>
	</joint>

	<link name="left_upper_arm">
		<visual>
			<geometry>
				<cylinder radius="0.04" length="0.8"/>
			</geometry>
			<origin xyz="0 0 0.3" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.04" length="0.8"/>
			</geometry>
			<origin xyz="0 0 0.3" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="1.5"/>
			<inertia ixx="0.015" iyy="0.015" izz="0.015" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<!-- Left Elbow -->
	<joint name="left_elbow" type="revolute">
		<parent link="left_upper_arm"/>
		<child link="left_lower_arm"/>
		<origin xyz="0 0 0.6" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<limit lower="-2.09" upper="2.09" effort="60" velocity="2.0"/>
	</joint>

	<link name="left_lower_arm">
		<visual>
			<geometry>
				<cylinder radius="0.03" length="0.5"/>
			</geometry>
			<origin xyz="0 0 0.25" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.03" length="0.5"/>
			</geometry>
			<origin xyz="0 0 0.25" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="0.8"/>
			<inertia ixx="0.008" iyy="0.008" izz="0.008" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<!-- Left Hand -->
	<joint name="left_hand_joint" type="fixed">
		<parent link="left_lower_arm"/>
		<child link="left_hand"/>
		<origin xyz="0 0 0.5" rpy="0 0 0"/>
	</joint>

	<link name="left_hand">
		<visual>
			<geometry>
				<box size="0.3 0.05 0.3"/>
			</geometry>
			<material name="red">
				<color rgba="1 0 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="0.3 0.05 0.3"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="0.1"/>
			<inertia ixx="0.0005" iyy="0.0005" izz="0.0005" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>


	<!-- RIGHT ARM -->

	<joint name="right_shoulder" type="revolute">
		<parent link="base_link"/>
		<child link="right_upper_arm"/>
		<origin xyz="-0.3 0 0.7" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<limit lower="-3.14" upper="3.14" effort="100" velocity="2.0"/>
	</joint>

	<link name="right_upper_arm">
		<visual>
			<geometry>
				<cylinder radius="0.04" length="0.8"/>
			</geometry>
			<origin xyz="0 0 0.3" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.04" length="0.8"/>
			</geometry>
			<origin xyz="0 0 0.3" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="1.5"/>
			<inertia ixx="0.015" iyy="0.015" izz="0.015" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<joint name="right_elbow" type="revolute">
		<parent link="right_upper_arm"/>
		<child link="right_lower_arm"/>
		<origin xyz="0 0 0.6" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<limit lower="-2.09" upper="2.09" effort="60" velocity="2.0"/>
	</joint>

	<link name="right_lower_arm">
		<visual>
			<geometry>
				<cylinder radius="0.03" length="0.5"/>
			</geometry>
			<origin xyz="0 0 0.25" rpy="0 0 0"/>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder radius="0.03" length="0.5"/>
			</geometry>
			<origin xyz="0 0 0.25" rpy="0 0 0"/>
		</collision>
		<inertial>
			<mass value="0.8"/>
			<inertia ixx="0.008" iyy="0.008" izz="0.008" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>

	<!-- Right Hand -->
	<joint name="right_hand_joint" type="fixed">
		<parent link="right_lower_arm"/>
		<child link="right_hand"/>
		<origin xyz="0 0 0.5" rpy="0 1.5708 0"/>
	</joint>

	<link name="right_hand">
		<visual>
			<geometry>
				<box size="0.3 0.05 0.3"/>
			</geometry>
			<material name="red">
				<color rgba="1 0 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="0.3 0.05 0.3"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="0.1"/>
			<inertia ixx="0.0005" iyy="0.0005" izz="0.0005" ixy="0" ixz="0" iyz="0"/>
		</inertial>
	</link>



</robot>